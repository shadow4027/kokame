{
    "name": "kokame",
    "version": "0.1",
    "description": "An extension to lessen all the busy work",
    "permissions": ["storage", "tabs"],
    
    "action": {
      "default_popup": "/html/popups/templateeditor.html"
    },
    "content_scripts": [
      {
        "matches": ["https://alder.pandolink.com/customer/master/*"],
        "js": ["notes_templates.js"]
      }
    ],
    "manifest_version": 3
  }